@model IEnumerable<Employee>
@{
    ViewBag.Title = "Employee List";
}
<div class="card-deck row justify-content-center my_scroll_div">
    @foreach (var employee in Model)
    {
        <partial name="_DisplayEmployeePartial" model="employee" />
    }
</div>
@section Script{
    <script src="~/js/CustomScript.js"></script>
}
